{
  "name": "react-advanced-click-away",
  "version": "2.1.6",
  "description": "A worry-free click away listener for advanced use cases.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src test stories --ext .ts,.tsx,.js,.jsx --cache --max-warnings 0",
    "cleanup": "npx rimraf dist storybook-static",
    "release": "standard-version",
    "prettier": "prettier --write '{src,test,stories}/**/*.{js,jsx,ts,tsx}'",
    "storybook": "sb dev -p 6006",
    "build:storybook": "npm run cleanup; NODE_ENV=production sb build"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "click away",
    "click outside",
    "react"
  ],
  "author": "Michal Obrocnik <roomy_western_0x@icloud.com>",
  "homepage": "https://mbixby.github.io/react-advanced-click-away/",
  "repository": {
    "url": "https://github.com/mbixby/react-advanced-click-away"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-terser": "^0.1.0",
    "@storybook/addon-actions": "^7.0.0-alpha.56",
    "@storybook/addon-docs": "^7.0.0-alpha.56",
    "@storybook/addon-essentials": "^7.0.0-alpha.56",
    "@storybook/addon-interactions": "^7.0.0-alpha.56",
    "@storybook/addon-links": "^7.0.0-alpha.56",
    "@storybook/blocks": "^7.0.0-alpha.56",
    "@storybook/cli": "^7.0.0-alpha.56",
    "@storybook/react": "^7.0.0-alpha.56",
    "@storybook/react-webpack5": "^7.0.0-alpha.56",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chroma-js": "^2.1.4",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.25",
    "@types/shallowequal": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-loader": "^9.1.0",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "lodash": "^4.17.21",
    "mapped-replace": "^1.0.3",
    "react-docgen-typescript": "^2.2.2",
    "react-icons": "^4.7.1",
    "react-popper": "^2.3.0",
    "remark-gfm": "^3.0.1",
    "rollup": "^3.5.1",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "shallowequal": "^1.1.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "root": true,
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "react/display-name": [
        "off"
      ],
      "@typescript-eslint/no-explicit-any": [
        "off"
      ],
      "@typescript-eslint/no-non-null-assertion": [
        "off"
      ],
      "@typescript-eslint/no-unnecessary-type-constraint": [
        "off"
      ],
      "@typescript-eslint/prefer-as-const": [
        "off"
      ],
      "@typescript-eslint/ban-ts-comment": [
        "off"
      ],
      "@typescript-eslint/no-empty-function": [
        "off"
      ],
      "react/no-unescaped-entities": [
        "off"
      ]
    }
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "fast-deep-equal": "^3.1.3"
  }
}
